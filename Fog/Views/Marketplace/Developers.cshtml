@model IEnumerable<DataLibrary.Models.DeveloperModel>
@{
    ViewData["Title"] = "Developer Lounge";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>Developer Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.ActionLink("Info", "DevInfo", "Developer", new { DevID = item.ID })
                </td>
            </tr>
        }
    </tbody>
</table>